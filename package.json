{
  "name": "mississippi.wtf",
  "version": "0.0.1",
  "description": "a twilio connection to your Mississippi senator",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:lint && npm run test:tape",
    "test:lint": "standard",
    "test:spec": "tape index.spec.js | tap-spec",
    "dev": "npm run dev:server & npm run dev:spec",
    "dev:server": "nodemon & browser-sync start --no-ghost-mode --proxy localhost:3000 --port 9000 --files 'public/**/*.*'",
    "dev:spec": "tape-watch index.spec.js | tap-spec",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@git.diff.mx:diffalot/contact-ms-senator.git"
  },
  "keywords": [
    "twilio",
    "sunlight"
  ],
  "author": "Andrew Davis <andrew@diff.mx>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.3.4",
    "dotenv": "^2.0.0",
    "hapi": "^13.2.2",
    "inert": "^3.2.0",
    "is-phone": "^2.0.0",
    "node-geocoder": "^3.9.1",
    "node-uuid": "^1.4.7",
    "openstates": "0.0.1",
    "twilio": "^2.9.1"
  },
  "devDependencies": {
    "browser-sync": "^2.11.2",
    "js-yaml": "^3.7.0",
    "nodemon": "^1.9.1",
    "standard": "^6.0.8",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "tape-watch": "^2.2.4"
  }
}
